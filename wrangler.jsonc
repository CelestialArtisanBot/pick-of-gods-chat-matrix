// Main Wrangler configuration for Pick of Gods Chat Matrix
{
  "name": "pick-of-gods-chat-matrix",
  "main": "src/index.ts",
  "compatibility_date": "2025-09-02",
  "ai_bindings": [
    {
      "name": "AI"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "AUTH_STORAGE",
      "id": "<your-auth-storage-id>"
    },
    {
      "binding": "READONLY",
      "id": "<your-readonly-id>"
    }
  ],
  "services": [
    {
      "binding": "DISPATCHER",
      "service": "DISPATCHER"
    }
  ],
  "vars": {
    "OPENROUTER_KEY": "",
    "CLOUDFLARE_ACCOUNT_ID": "<your-account-id>"
  },
  "env": {
    "development": {},
    "production": {}
  }
}
